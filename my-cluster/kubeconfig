kind: Config
clusters:
- cluster:
    certificate-authority: credentials/ca.pem
    server: https://k8ssandbox
  name: kube-aws-k8ssandbox-cluster
contexts:
- context:
    cluster: kube-aws-k8ssandbox-cluster
    namespace: default
    user: kube-aws-k8ssandbox-admin
  name: kube-aws-k8ssandbox-context
users:
- name: kube-aws-k8ssandbox-admin
  user:
    client-certificate: credentials/admin.pem
    client-key: credentials/admin-key.pem
current-context: kube-aws-k8ssandbox-context
